public class Ciclo_If{
	public static void main(String args[]){
		int matematicas=5;
		int biologia=9;
		int historia=6;
		double promedio=(matematicas+biologia+historia)/3;
		if(promedio>=6){
			System.out.println("Aprobaste con: "+promedio+" perro");
		}else{
			System.out.println("Valiste verga papu");
		}

	}
}
